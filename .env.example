# LinkedIn_Apply environment variables

# Credentials (do not commit real values)
LINKEDIN_EMAIL=
LINKEDIN_PASSWORD=

# Browser/profile settings
HEADLESS=false
CHROME_PROFILE_PATH="$HOME/.config/LinkedIn_Apply_Profile"
# Optional: If CHROME_PROFILE_PATH points to the base dir, pick a profile subdir (e.g., "Default", "Profile 1")
CHROME_PROFILE_DIR="Default"

# Applicant info used to auto-fill Easy Apply forms
PHONE=
CITY=
STATE=
ZIP=
COUNTRY=
ADDRESS=
PORTFOLIO_URL=
GITHUB_URL=
WEBSITE_URL=
RESUME_PATH=

# Common Yes/No questions
WORK_AUTH=yes
SPONSORSHIP=no
RELOCATE=no

# Other fields
EXPECTED_SALARY=
START_DATE=
YEARS_EXPERIENCE=

